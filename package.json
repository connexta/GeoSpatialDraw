{
  "name": "geospatialdraw",
  "version": "0.4.6",
  "description": "Geospatial Map Drawing Library",
  "types": "./target/webapp/index.d.ts",
  "main": "./target/webapp/index.js",
  "author": "Connexta LLC",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/connexta/geospatialdraw",
  "keywords": [
    "catography",
    "coordinate",
    "coordinates",
    "drawing",
    "geospatial",
    "gps",
    "lat/long",
    "latitude",
    "longitude",
    "map",
    "mapping",
    "mgrs",
    "primative",
    "usng",
    "utm"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/connexta/geospatialdraw.git"
  },
  "engines": {
    "node": ">=0.10.5"
  },
  "scripts": {
    "fmt": "ace format -w",
    "prestart": "ace disable-idp",
    "pretest": "ace bundle --env=test",
    "test": "ace test",
    "start": "yarn storybook",
    "start:test": "ace start --env=test",
    "storybook": "ace storybook",
    "prebuild": "rimraf ./target",
    "build": "tsc -p tsconfig.json",
    "build:docs": "ace storybook --static && typedoc",
    "build:verify": "yarn run fmt && yarn run test && yarn run build",
    "build:all": "yarn run build:verify && yarn run build:docs",
    "prepublish": "yarn build:all",
    "m2": "yarn install:m2",
    "install:m2": "ace clean && ace bundle && ace package && ace install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.21",
    "@fortawesome/free-solid-svg-icons": "5.10.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@turf/buffer": "6.5.0",
    "@turf/nearest-point-to-line": "6.0.0",
    "@turf/point-to-line-distance": "6.0.0",
    "@turf/turf": "5.1.6",
    "@types/openlayers": "4.3.11",
    "font-awesome": "4.7",
    "lodash": "4.17.15",
    "openlayers": "4.6.5",
    "polished": "1.9.3",
    "react": "16.8.6",
    "react-ace": "6.1.3",
    "react-dom": "16.8.6",
    "usng.js": "0.4.2"
  },
  "devDependencies": {
    "@connexta/ace": "git://github.com/connexta/ace.git#235d5b3885e1525e2f42bfbd64cd9715d85f498c",
    "@emotion/core": "10.0.16",
    "@emotion/styled": "10.0.15",
    "@types/lodash": "4.14.137",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.8.4",
    "@types/react-intl": "2.3.12",
    "@types/styled-components": "4.1.8",
    "chai": "4.2.0",
    "emotion": "10.0.14",
    "emotion-theming": "10.0.14",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "prismjs": "1.17.1",
    "react-popper": "1.3.4",
    "react-syntax-highlighter": "11.0.2",
    "react-textarea-autosize": "7.1.0",
    "recompose": "0.30.0",
    "rimraf": "3.0.0",
    "styled-components": "4.3.2",
    "typedoc": "0.15.0",
    "typescript-bundle": "1.0.16"
  },
  "peerDependencies": {
    "styled-components": ">= 4"
  }
}
